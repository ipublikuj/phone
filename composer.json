{
  "name" : "ipub/phone",
  "type" : "library",
  "description" : "Phone helper & validator for Nette Framework",
  "keywords" : [
    "nette",
    "phone",
    "tools",
    "ipub",
    "ipublikuj"
  ],

  "homepage" : "https://github.com/ipublikuj/phone",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "http://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/ipublikuj/phone/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extension" : "IPub\\Phone\\DI\\PhoneExtension"
      }
    }
  },

  "require" : {
    "php" : ">=7.4.0",
    "ext-intl" : "*",
    "ext-ctype" : "*",
    "giggsey/libphonenumber-for-php" : "^8.12",
    "nette/di" : "^3.0",
    "nette/utils" : "^3.2"
  },

  "require-dev" : {
    "contributte/console": "^0.9",
    "mockery/mockery": "^1.4",
    "nette/bootstrap": "^3.1",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "pds/skeleton": "~1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "tracy/tracy": "^2.8"
  },

  "autoload" : {
    "psr-4" : {
      "IPub\\Phone\\" : "src/"
    }
  },

  "config" : {
    "sort-packages" : true
  }
}
